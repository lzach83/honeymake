{
  "name": "honeymake",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "initialize": "sh ./scripts/init.sh",
    "start:services": "sh ./scripts/start-services.sh",
    "clean:docker": "sh ./scripts/clean-docker.sh",
    "build:packages": "sh ./scripts/build-packages.sh",
    "build:services": "sh ./scripts/build-services.sh",
    "deploy:services": "lerna run deploy",
    "new-version": "lerna version --conventional-commits --yes --no-changelog",
    "ci:build": "lerna run build",
    "ci:test": "lerna run ci:test",
    "ci:deploy": "sh ./scripts/deploy.sh",
    "new:service": "npx hygen service new",
    "new:package": "npx hygen package new"
  },
  "dependencies": {
    "react-native": "0.66.3",
    "@honeymake/api": "1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "concurrently": "^5.3.0",
    "copyfiles": "^2.4.0",
    "husky": "^4.3.0",
    "hygen": "^6.0.4",
    "lerna": "^4.0.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "ts-jest": "^26.4.3",
    "tslint": "^6.1.3",
    "typescript": "^4.0.5"
  },
  "workspaces": {
    "packages": [
      "services/*",
      "frontend/*"
    ]
  }
}
